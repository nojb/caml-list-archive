<script>

function validenews(){	

if (document.getElementById("invit-email").value=="" || document.getElementById("invit-nom").value=="" ){

alert('Veuillez indiquer Votre nom et une adresse e-mail correcte..!');	

document.getElementById("invit-nom").style.backgroundColor='#FED6B8';		

document.getElementById("invit-email").style.backgroundColor='#FED6B8';								

}else{	

m = document.getElementById("invit-email").value;

//alert(m);

valide = false;

for(var j=1;j<(m.length);j++){

	if(m.charAt(j)=='@'){

		if(j<(m.length-4)){

			for(var k=j;k<(m.length-2);k++){

				if(m.charAt(k)=='.') valide=true;

			}

		}

	}

}

if(valide){

document.getElementById("cmdinvit").value="Valider";

document.forms["invitation"].submit();

}else{

	alert('Veuillez indiquer une adresse e-mail correcte..!');

	document.getElementById("invit-email").style.backgroundColor='#FED6B8';	

	document.getElementById("invit-email").focus();

			}

}

}

function getfoc() {

document.getElementById("invit-nom").focus();

}

document.onload = getfoc();

</script></td></tr><tr><td><br><samp style="font-family: Verdana, Arial, Helvetica, sans-serif;font-weight: bold;font-size: 10px;" ></samp><br></td></tr></table>

